<% block_me nil do %>
  <% title @project.title %> 
  <%= link_to @project.cat.name.downcase, {:action => :view_category, :id => @project.cat} %>
  <h1><%= @project.title %></h1>
  <%= render_partial "support_project" %>
  <p>
    Project Artist: <%= link_to @owner.full_name, :controller => :profile, :action => :view, :id => @owner.id %> 
  </p>
  <p> 
    <%= @project.formatted_description %>
  </p>
<% end %>