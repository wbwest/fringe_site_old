<% block_me "fringe projects" do %>
  <%  @projects.each do |project|  %>
  <p>
    <%= link_to project.title, :action => :view, :id => project.id %>
    <br/>
    by <%= project.user.full_name %>
    (<%= project.votes.length %> supporters)
  </p>
  <%  end  %>
  <%= will_paginate @projects %>
<% end %>