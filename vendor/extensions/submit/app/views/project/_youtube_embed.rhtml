<%  
  width = "425" if local_assigns[:width].nil? 
  height = "355" if local_assigns[:height].nil? 
%>

<object width=<%= width %> height=<%= height %>
  <param name="movie" value="http://www.youtube.com/v/<%= youtube_id %> &rel=1"></param>
  <param name="wmode" value="transparent"></param>
  <embed src="http://www.youtube.com/v/<%= youtube_id %>&rel=1" type="application/x-shockwave-flash" wmode="transparent"  width=<%= width %> height=<%= height %></embed>
</object>