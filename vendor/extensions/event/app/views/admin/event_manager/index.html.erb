<h1>Events</h1>

<%= link_to "Add Event", :action => :new %>
<br/><br/>

<table class="admin_table">
  <% @events.each do |event| %>
  <tr>
    <td><%= link_to event.name, :action=>:view, :id=>event %></td>
    <td><%= link_to "edit", :action=>:edit, :id=>event %></td>
    <td><%= link_to "delete", {:action=>:delete, :id=>event}, :confirm=>"Are you sure?" %>
  </tr>
  
  <% end %>
</table>
